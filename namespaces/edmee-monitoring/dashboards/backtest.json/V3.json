            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "SELECT \r\n    t.id AS trade_id,\r\n    t.entryTs AS entryTimestamp,\r\n    t.closeTs AS closeTimestamp,\r\n    t.type AS trade_type,\r\n    TIMESTAMPDIFF(HOUR, t.entryTs, t.closeTs) AS duration_hours,\r\n    t.entryPrice,\r\n    t.closePrice,\r\n    ROUND(\r\n        CASE \r\n            WHEN t.type = 'LONG' THEN ((t.closePrice - t.entryPrice) / t.entryPrice) * ${leverage} * 100 - ((${leverage}*2*0.1) + 0.1)\r\n            WHEN t.type = 'SHORT' THEN ((t.entryPrice - t.closePrice) / t.entryPrice) * ${leverage} * 100 - ((${leverage}*2*0.1) + 0.1)\r\n        END, 2\r\n    ) AS profit_percentage\r\nFROM \r\n    BT_trades t\r\nJOIN \r\n    BT_strategies s ON t.strategyId = s.id\r\nJOIN \r\n    BT_backtests b ON s.backtestId = b.id\r\nWHERE \r\n    b.name = '${backtest_name}'\r\n    AND s.id = '${strategy_id}';\r\n",
            "refId": "A",
            "sql": {
