            "format": "table",
            "rawQuery": true,
            "rawSql": "SELECT\r\n  CASE\r\n    WHEN profit_percentage < -20 THEN \"< -20%\"\r\n    WHEN profit_percentage BETWEEN -20 AND -15 THEN \"-20% à -15%\"\r\n    WHEN profit_percentage BETWEEN -15 AND -10 THEN \"-15% à -10%\"\r\n    WHEN profit_percentage BETWEEN -10 AND -5 THEN \"-10% à -5%\"\r\n    WHEN profit_percentage BETWEEN -5 AND 0 THEN \"-5% à 0%\"\r\n    WHEN profit_percentage BETWEEN 0 AND 5 THEN \"0% à 5%\"\r\n    WHEN profit_percentage BETWEEN 5 AND 10 THEN \"5% à 10%\"\r\n    WHEN profit_percentage BETWEEN 10 AND 15 THEN \"10% à 15%\"\r\n    WHEN profit_percentage BETWEEN 15 AND 20 THEN \"15% à 20%\"\r\n    WHEN profit_percentage BETWEEN 20 AND 30 THEN \"20% à 30%\"\r\n    ELSE \"> 30%\"\r\n  END AS profit_range,\r\n  COUNT(*) AS trade_count,\r\n  CASE\r\n    WHEN profit_percentage < -20 THEN 1\r\n    WHEN profit_percentage BETWEEN -20 AND -15 THEN 2\r\n    WHEN profit_percentage BETWEEN -15 AND -10 THEN 3\r\n    WHEN profit_percentage BETWEEN -10 AND -5 THEN 4\r\n    WHEN profit_percentage BETWEEN -5 AND 0 THEN 5\r\n    WHEN profit_percentage BETWEEN 0 AND 5 THEN 6\r\n    WHEN profit_percentage BETWEEN 5 AND 10 THEN 7\r\n    WHEN profit_percentage BETWEEN 10 AND 15 THEN 8\r\n    WHEN profit_percentage BETWEEN 15 AND 20 THEN 9\r\n    WHEN profit_percentage BETWEEN 20 AND 30 THEN 10\r\n    ELSE 11\r\n  END AS range_order\r\nFROM (\r\n  SELECT\r\n    CASE\r\n      WHEN type = \"LONG\" THEN ((closePrice - entryPrice) / entryPrice) * 100 * ${leverage} - ((${leverage}+1)*0.15 + 0.1)\r\n      WHEN type = \"SHORT\" THEN ((entryPrice - closePrice) / entryPrice) * 100 * ${leverage} - ((${leverage}+1)*0.15 + 0.1)\r\n    END AS profit_percentage\r\n  FROM\r\n    BT_trades\r\n  WHERE\r\n    strategyId = '${strategy_id}'\r\n) AS profits\r\nGROUP BY profit_range, range_order\r\nORDER BY range_order;\r\n",
