        "targets": [
          {
            "dataset": "backtest",
            "datasource": {
              "type": "mysql",
              "uid": "${DS_MYSQL-BACKTEST-BDD}"
            },
            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "SELECT \r\n    s.id,\r\n    s.timeframe,\r\n    s.coeff0,\r\n    s.coeff1,\r\n    s.coeff2,\r\n    s.coeff3,\r\n    s.coeff4,\r\n    COUNT(t.id) AS trade_count,\r\n    ROUND(STDDEV(\r\n        CASE \r\n            WHEN t.type = 'LONG' THEN ((t.closePrice - t.entryPrice) / t.entryPrice) * ${leverage} * 100 - ((${leverage}*2*0.1) + 0.1)\r\n            WHEN t.type = 'SHORT' THEN ((t.entryPrice - t.closePrice) / t.entryPrice) * ${leverage} * 100 - ((${leverage}*2*0.1) + 0.1)\r\n        END\r\n    ), 2) AS profit_stddev,\r\n    ROUND(AVG(\r\n        CASE \r\n            WHEN t.type = 'LONG' THEN ((t.closePrice - t.entryPrice) / t.entryPrice) * ${leverage} * 100 - ((${leverage}*2*0.1) + 0.1)\r\n            WHEN t.type = 'SHORT' THEN ((t.entryPrice - t.closePrice) / t.entryPrice) * ${leverage} * 100 - ((${leverage}*2*0.1) + 0.1)\r\n        END\r\n    ), 2) AS avg_profit_percentage,\r\n    ROUND(SUM(\r\n        CASE \r\n            WHEN t.type = 'LONG' THEN ((t.closePrice - t.entryPrice) / t.entryPrice) * ${leverage} * 100 - ((${leverage}*2*0.1) + 0.1)\r\n            WHEN t.type = 'SHORT' THEN ((t.entryPrice - t.closePrice) / t.entryPrice) * ${leverage} * 100 - ((${leverage}*2*0.1) + 0.1)\r\n        END\r\n    ), 2) AS total_relative_profit_ajusted,\r\n    ROUND(SUM(\r\n        CASE \r\n            WHEN t.type = 'LONG' THEN ((t.closePrice - t.entryPrice) / t.entryPrice) * 100 \r\n            WHEN t.type = 'SHORT' THEN ((t.entryPrice - t.closePrice) / t.entryPrice) * 100 \r\n        END\r\n    ), 2) AS total_relative_profit\r\nFROM \r\n    BT_strategies s\r\nLEFT JOIN \r\n    BT_trades t ON s.id = t.strategyId\r\nWHERE \r\n    s.backtestId = (SELECT id FROM BT_backtests WHERE name = '${backtest_name}')\r\n    AND s.symbol = '${symbol}'\r\nGROUP BY \r\n    s.id\r\nORDER BY\r\n    total_relative_profit DESC\r\nLIMIT 100;\r\n",
            "refId": "A",
            "sql": {
              "columns": [
                {
                  "parameters": [],
                  "type": "function"
                }
              ],
              "groupBy": [
                {
                  "property": {
                    "type": "string"
                  },
                  "type": "groupBy"
                }
              ],
              "limit": 50
            },
