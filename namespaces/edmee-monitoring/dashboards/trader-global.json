            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "SELECT \r\n    closeTimestamp,\r\n    @cumulative_profit := @cumulative_profit + profit_usd AS cumulative_profit_usd\r\nFROM (\r\n    SELECT \r\n        t.closeTs AS closeTimestamp,\r\n        ROUND(\r\n            CASE \r\n                WHEN t.type = 'LONG' THEN (t.closePrice * t.quantity) - (t.entryPrice * t.quantity)\r\n                WHEN t.type = 'SHORT' THEN (t.entryPrice * t.quantity) - (t.closePrice * t.quantity)\r\n            END, 2) AS profit_usd\r\n    FROM \r\n        T_trades t\r\n    JOIN \r\n        T_strategies s ON t.id_strategy = s.id\r\n    JOIN \r\n        T_users u ON s.userId = u.id\r\n    WHERE \r\n        u.username = '${user}'\r\n        AND t.closeTs IS NOT NULL -- Considérer uniquement les trades clôturés\r\n    ORDER BY \r\n        t.closeTs ASC\r\n) AS profits, (SELECT @cumulative_profit := 0) AS init;\r\n",
            "refId": "A",
