            "rawSql": "\r\n\r\nSELECT \r\n    ROUND(AVG(\r\n        CASE \r\n            WHEN t.type = 'LONG' THEN ((t.closePrice - t.entryPrice) / t.entryPrice) * 100 * s.leverage\r\n            WHEN t.type = 'SHORT' THEN ((t.entryPrice - t.closePrice) / t.entryPrice) * 100 * s.leverage\r\n        END), 2) AS avg_profit_percentage\r\nFROM \r\n    T_trades t\r\nJOIN \r\n    T_strategies s ON t.id_strategy = s.id\r\nJOIN \r\n    T_users u ON s.userId = u.id\r\nWHERE \r\n    t.closePrice IS NOT NULL \r\n    AND u.username = '${user}';\r\n",
