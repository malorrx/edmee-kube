            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "SELECT \r\n    t.symbol,\r\n    t.entryTs,\r\n    t.closeTs,\r\n    t.entryPrice,\r\n    t.closePrice,\r\n    t.quantity,\r\n    t.type,\r\n    ROUND(\r\n        CASE \r\n            WHEN t.type = 'LONG' THEN (t.closePrice * t.quantity) - (t.entryPrice * t.quantity)\r\n            WHEN t.type = 'SHORT' THEN (t.entryPrice * t.quantity) - (t.closePrice * t.quantity)\r\n        END, 2) AS profit_usd,\r\n    ROUND(TIMESTAMPDIFF(SECOND, t.entryTs, t.closeTs) / 3600, 2) AS duration_hours,\r\n    ROUND(\r\n        CASE \r\n            WHEN t.type = 'LONG' THEN ((t.closePrice - t.entryPrice) / t.entryPrice) * 100\r\n            WHEN t.type = 'SHORT' THEN ((t.entryPrice - t.closePrice) / t.entryPrice) * 100\r\n        END, 2) AS profit_percentage\r\nFROM \r\n    T_trades t\r\nJOIN \r\n    T_strategies s ON t.id_strategy = s.id\r\nJOIN \r\n    T_users u ON s.userId = u.id\r\nWHERE \r\n    u.username = '${user}';\r\n\r\n\r\n",
            "refId": "A",
            "sql": {
              "columns": [
                {
                  "parameters": [],
                  "type": "function"
                }
