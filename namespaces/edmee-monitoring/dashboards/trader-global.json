            },
            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "SELECT \r\n    ROUND((SUM(\r\n        CASE \r\n            WHEN (t.type = 'LONG' AND t.closePrice > t.entryPrice) OR\r\n                 (t.type = 'SHORT' AND t.entryPrice > t.closePrice)\r\n            THEN 1 ELSE 0 END\r\n    ) / COUNT(*)) * 100, 2) AS positive_trade_percentage\r\nFROM \r\n    T_trades t\r\nJOIN \r\n    T_strategies s ON t.id_strategy = s.id\r\nJOIN \r\n    T_users u ON s.userId = u.id\r\nWHERE \r\n    t.closePrice IS NOT NULL \r\n    AND u.username = '${user}';\r\n",
            "refId": "A",
            "sql": {
              "columns": [
                {
                  "parameters": [],
                  "type": "function"
                }
